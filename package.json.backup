{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "backend:dev": "wrangler dev",
    "backend:deploy": "bash scripts/deploy-backend.sh",
    "backend:logs": "wrangler tail",
    "db:init": "wrangler d1 execute meownocode --file=d1-schema.sql",
    "db:backup": "wrangler d1 export meownocode --output=backup.sql",
    "fullstack:dev": "concurrently \"npm run dev\" \"npm run backend:dev\"",
    "setup": "npm install && npm run backend:deploy"
  },
